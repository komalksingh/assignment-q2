   "source": [
    "## Python code to reverse a string  \n",
    "\n",
    "  \n",
    "\n",
    "def reverse(s): \n",
    "\n",
    "  str = \"\" \n",
    "\n",
    "  for i in s: \n",
    "\n",
    "    str = i + str\n",
    "\n",
    "  return str\n",
    "\n",
    "  \n",
    "\n",
    "s = \"1234abcd\"\n",
    "\n",
    "  \n",
    "\n",
    "print (\"The original string  is : \",end=\"\") \n",
    "\n",
    "print (s) \n",
    "\n",
    "  \n",
    "\n",
    "print (\"The reversed string(using loops) is : \",end=\"\")\n",
    "print(reverse(s))"
   ]
